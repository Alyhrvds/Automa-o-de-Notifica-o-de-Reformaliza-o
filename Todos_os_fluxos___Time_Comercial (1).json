    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  // Se não houver campo binário, retorna o item como está\n  if (!item.binary) {\n    return item;\n  }\n\n  const oldKey = Object.keys(item.binary)[0];\n\n  // Clona o binário para \"data\"\n  item.binary[\"data\"] = item.binary[oldKey];\n\n  // Renomeia\n  item.binary[\"data\"].fileName = \"indicadores_renomeado.png\";\n  item.binary[\"data\"].mimeType = \"image/png\";\n  item.binary[\"data\"].fileExtension = \"png\";\n\n  // Remove o binário original (ex: se era \"file\" ou outro nome)\n  if (oldKey !== \"data\") {\n    delete item.binary[oldKey];\n  }\n\n  return item;\n});"
      },
